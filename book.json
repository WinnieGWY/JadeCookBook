{
    "plugins": [
        "exercises",
        "prism",
        "github-buttons",
        "duoshuo",
        "emphasize",
        "-highlight",
        "github",
        "ga",
        "editlink"
    ],
    "pluginsConfig": {
        "duoshuo": {
            "short_name": "jadeterry",
            "theme": "default"
        },
        "github-buttons": {
            "repo": "terrynie/JadeCookBook",
            "types": [
                "star",
                "watch"
            ]
        },
        "github": {
            "url": "https://github.com/terrynie/JadeCookBook"
        },
        "ga": {
            "token": "UA-75642672-1"
        },
        "editlink": {
            "base": "https://github.com/terrynie/JadeCookBook/blob/master/",
            "label": {
                "en": "Edit This Page",
                "zh-cn": "编辑本页"
            },
            "multilingual": true
        }
     }
}